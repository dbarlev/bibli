{
  "name": "bibli",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^1.2.25",
    "axios": "^0.18.0",
    "docx": "^5.0.0-rc7",
    "file-saver": "^2.0.2",
    "locutus": "^2.0.10",
    "node-sass-chokidar": "^1.3.4",
    "npm-run-all": "^4.1.3",
    "react": "^16.4.1",
    "react-animated-css": "^1.2.1",
    "react-bootstrap": "^0.32.1",
    "react-bootstrap-combobox": "^3.0.1",
    "react-bootstrap-select": "^0.14.0",
    "react-bootstrap-tabs": "^1.0.2",
    "react-dom": "^16.4.1",
    "react-redux": "^5.0.7",
    "react-router-bootstrap": "^0.24.4",
    "react-router-dom": "^4.3.1",
    "react-scripts": "2.0.3",
    "react-select": "^2.0.0",
    "reactstrap": "^6.4.0",
    "redux": "^4.0.0",
    "redux-thunk": "^2.3.0"
  },
  "homepage": "/",
  "scripts": {
    "build-css": "node-sass-chokidar --include-path ./public --include-path ./node_modules public/ -o public/",
    "watch-css": "npm run build-css && node-sass-chokidar --include-path ./public --include-path ./node_modules public/ -o public/ --watch --recursive",
    "start-js": "react-scripts start",
    "build": "npm run build-css && react-scripts build",
    "start": "npm-run-all -p watch-css start-js",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject"
  },
  "devDependencies": {
    "case-sensitive-paths-webpack-plugin": "^2.1.2"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ]
}
